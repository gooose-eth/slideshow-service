    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "nodemon": "^2.0.22",
    "pinia": "^2.1.4",
    "sass": "^1.63.6",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  }
