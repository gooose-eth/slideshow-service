{
  "name": "slideshow.redgoose.me",
  "version": "3.0.0",
  "repository": "https://github.com/redgoose-dev/slideshow.redgoose.me.git",
  "author": "redgoose <scripter@me.com>",
  "type": "module",
  "license": "ISC",
  "scripts": {
    "dev": "node --no-warnings server/entry.js --development",
    "dev:live": "nodemon --no-warnings server/entry.js --development",
    "prebuild": "rm -rf ./dist",
    "build": "vite build",
    "start": "node --no-warnings server/entry.js --production",
    "start:live": "nodemon --no-warnings server/entry.js --production"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": ["src/**/*", "dist/**/*"]
  },
  "engines": {
    "node": "16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "isbot": "^3.6.3",
    "ohmyfetch": "^0.4.21",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "nodemon": "^2.0.20",
    "pinia": "^2.0.23",
    "sass": "^1.56.0",
    "vite": "^3.2.2",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  }
}